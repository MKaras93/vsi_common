#!/usr/bin/env bash
set -eu
VSI_COMMON_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")/.."; pwd)"

. "${VSI_COMMON_DIR}/linux/cuda_info.bsh"

[ "${CUDA_ARCHES+set}" != "set" ]
[ "${CUDA_CODES+set}" != "set" ]
[ "${CUDA_SUGGESTED_ARCHES+set}" != "set" ]
[ "${CUDA_SUGGESTED_CODES+set}" != "set" ]
[ "${CUDA_SUGGESTED_PTX+set}" != "set" ]
[ "${CUDA_VERSIONS+set}" != "set" ]
[ "${CUDA_VERSION}" = "" ]
[ "${CUDA_FORWARD_PTX}" = "" ]

echo "1"